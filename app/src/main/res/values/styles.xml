<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="MyBaseAppTheme">
        <!-- Customize your theme here. -->
    </style>

    <style name="MyBaseAppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/default_text</item>
        <item name="colorPrimaryDark">@color/default_text</item>
        <item name="colorAccent">@color/default_text</item>
        <item name="android:textColorPrimary">@color/default_text</item>
    </style>

    <style name="pop_voice">
        <item name="android:windowEnterAnimation">@anim/pop_voice_in</item>
        <item name="android:windowExitAnimation">@anim/pop_voice_out</item>
    </style>

    <style name="pro_item5_add">
        <item name="android:windowEnterAnimation">@anim/pro_item5_add_in</item>
        <item name="android:windowExitAnimation">@anim/pro_item5_ad_out</item>
    </style>

    <style name="setting_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cal_text</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
        <item name="android:background">@drawable/edittext_bg_select</item>
    </style>

    <style name="notification_cart">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
        <item name="android:textColor">@color/cal_text</item>
    </style>

    <style name="notification_checkbox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:button">@drawable/check_cb_select</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/text_size_item2</item>
        <item name="android:textColor">@color/gray_text</item>
        <item name="android:background">@drawable/edittext_bg_select</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:paddingStart">10dp</item>
    </style>


    <style name="account_tv_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
        <item name="android:textColor">@color/cal_text</item>
    </style>

    <style name="account_et_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">@dimen/text_size_item2</item>
        <item name="android:textColor">@color/gray_text</item>
        <item name="android:lines">1</item>
        <item name="android:background">@drawable/edittext_bg_select</item>
        <item name="android:enabled">false</item>
    </style>


    <style name="main_iv_item_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scaleType">center</item>
    </style>

    <style name="per_name_expanded">
        <item name="android:textSize">15sp</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="pro_name_expanded">
        <item name="android:textSize">20sp</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="per_name_collapsed">
        <item name="android:textSize">12sp</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="per_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="per_tv_name">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">8dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_item2</item>
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="per_tv_group">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">8dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="per_tv_time">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="per_ll_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginStart">25dp</item>
        <item name="android:layout_marginEnd">30dp</item>
    </style>

    <style name="ach_tv_type">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cal_text</item>
    </style>

    <style name="ach_tv_number">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/default_text</item>
        <item name="android:textSize">40sp</item>
    </style>

    <style name="ach_tv_number_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cal_text</item>
    </style>

    <style name="per_iv_task_details">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="main_add_tv_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:visibility">invisible</item>
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="main_add_ll_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="main_add_iv_pic">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="add_iv_menu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginStart">10dp</item>
    </style>

    <style name="pro_btn_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">3dp</item>
        <item name="android:textColor">@color/transparent_black</item>
        <item name="android:textSize">@dimen/text_size_item2</item>
    </style>

    <style name="log_ev">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginEnd">60dp</item>
        <item name="android:layout_marginStart">60dp</item>
        <item name="android:background">@drawable/edittext_bg_select</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">20</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:textColorHint">@color/gray_text</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="log_bt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="pro_btn_layout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="pro_btn_image">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="pub_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="pub_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@color/default_text</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
    </style>

    <style name="pub_tv_right">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">end</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@color/transparent_black</item>
        <item name="android:textSize">@dimen/text_size_item1</item>
    </style>

    <style name="pub_image">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">3dp</item>
        <item name="android:scaleType">fitEnd</item>
    </style>

</resources>
